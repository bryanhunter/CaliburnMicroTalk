<Application xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="GameLibrary.App"
             xmlns:local="clr-namespace:GameLibrary">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/DefaultStyles.xaml" />
				<ResourceDictionary Source="Resources/NamedStyles.xaml" />
			</ResourceDictionary.MergedDictionaries>

			<local:MefBootstrapper x:Key="bootstrapper" />

		</ResourceDictionary>

	</Application.Resources>
</Application>